{% extends 'base.html.twig' %}

{% block title %}Éditer le Tricount{% endblock %}

{% block body %}
    <h1>Éditer le Tricount</h1>

    {{ form_start(form) }}

    {{ form_row(form.name) }}
    {{ form_row(form.description) }}
    {{ form_row(form.category) }}
    {{ form_row(form.user) }}

    <button type="submit" class="btn btn-primary">Enregistrer</button>

    {{ form_end(form) }}

    <a href="{{ path('app_tricount', {'tricountId': tricount.id}) }}">Retour au Tricount</a>
{% endblock %}
